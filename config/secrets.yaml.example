# Copy to secrets.yaml (do not commit)

schwab:
  app_key: "YOUR_APP_KEY"
  app_secret: "YOUR_APP_SECRET"
  callback_url: "https://127.0.0.1"
  token_path: "./config/schwab_token.json"

campfire:
  bot_key: ""  # or api_token

# Tradovate API for automated order execution
# Get your API key at: Tradovate Settings → API Access → Generate API Key
tradovate:
  username: "your_tradovate_username"
  password: "your_tradovate_password"
  cid: "your_client_id"           # From API key generation
  secret: "your_api_secret"       # From API key generation
  app_id: "ORB-Gooey-Bot"
  environment: "demo"             # "demo" or "live"
